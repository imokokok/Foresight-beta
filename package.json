{
  "name": "forsight",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*",
    "infra/*",
    "scripts"
  ],
  "scripts": {
    "preinstall": "node -e \"const major=Number((process.versions.node||'').split('.')[0]); if(major!==20){console.error('Node.js 20 required. Current:', process.versions.node); process.exit(1)}\"",
    "postinstall": "node -e \"const fs=require('fs');const p='node_modules/next/dist/compiled/@vercel/nft/index.js';if(!fs.existsSync(p))process.exit(0);let s=fs.readFileSync(p,'utf8');const from='r[\\\"import.meta\\\"]={url:Q};const s=await(0,c.evaluate)(e,r,t);return s';const to='r[\\\"import.meta\\\"]={url:Q};let s;try{s=await(0,c.evaluate)(e,r,t)}catch(e){if(e&&e.name===\\\"RangeError\\\"&&String(e.message||\\\"\\\").includes(\\\"Division by zero\\\"))return undefined;throw e}return s';if(s.includes(to))process.exit(0);if(!s.includes(from))process.exit(0);fs.writeFileSync(p,s.replace(from,to));\"",
    "dev:web": "dotenv -e ./.env.local -e ./apps/web/.env.local -- npm run dev -w apps/web",
    "build:web": "npm run build -w apps/web",
    "start:web": "npm run start -w apps/web",
    "dev:relayer": "dotenv -e ./.env -e ./.env.local -- npm run start -w services/relayer",
    "build:relayer": "npm run build -w services/relayer",
    "test:web": "npm run test:run -w apps/web",
    "test:contracts": "npm run hardhat:test",
    "build:contracts": "npm run hardhat:compile",
    "dev": "concurrently -n web,relayer -c green,cyan \"npm run dev:web\" \"npm run dev:relayer\"",
    "build": "npm run build:web && npm run build:relayer",
    "start": "npm run start:web",
    "migrate:supabase": "tsx scripts/migrate-to-supabase.ts",
    "hardhat:compile": "cross-env HARDHAT_DISABLE_TELEMETRY_PROMPT=true hardhat compile",
    "hardhat:test": "cross-env HARDHAT_DISABLE_TELEMETRY_PROMPT=true hardhat test",
    "hardhat:node": "hardhat node",
    "hardhat:deploy": "hardhat run scripts/deploy.ts --network localhost",
    "hardhat:deploy:sepolia": "hardhat run scripts/deploy.ts --network sepolia",
    "hardhat:verify": "hardhat verify --network sepolia",
    "ws:dev": "npm run dev:web",
    "ws:build": "npm run build:web",
    "ws:contracts:compile": "npm run hardhat:compile",
    "ws:relayer:start": "npm run dev:relayer",
    "ws:relayer:build": "npm run build:relayer",
    "ws:test": "npm run hardhat:test",
    "ws:dev:all": "npm run dev",
    "lint": "npm run lint -w apps/web",
    "typecheck": "npm run typecheck -w apps/web",
    "test": "npm run test:web",
    "check:web": "npm run lint && npm run typecheck && npm run test",
    "check:relayer": "npm run typecheck -w services/relayer && npm test -w services/relayer",
    "check:contracts": "npm run hardhat:test",
    "seed:proposals:api": "node scripts/seed-proposals-via-api.js",
    "check:all": "npm run check:web && npm run check:relayer && npm run check:contracts",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write"
    ],
    "**/*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "@react-spring/web": "^10.0.3",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^5.90.5",
    "@use-gesture/react": "^10.3.1",
    "abdk-libraries-solidity": "^3.2.0",
    "dotenv": "^17.2.3",
    "ethers": "^6.15.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.545.0",
    "next": "15.5.9",
    "node-fetch": "^3.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "siwe": "^3.0.0",
    "solmate": "^6.8.0",
    "viem": "^2.38.3",
    "wagmi": "^2.18.1",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.2",
    "@nomicfoundation/hardhat-ignition": "^0.15.15",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.16",
    "@nomicfoundation/hardhat-network-helpers": "^1.1.2",
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@nomicfoundation/hardhat-verify": "^2.1.1",
    "@nomicfoundation/ignition-core": "^0.15.14",
    "@tailwindcss/postcss": "^4",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "chai": "^4.5.0",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.4",
    "hardhat": "^2.26.3",
    "hardhat-gas-reporter": "^2.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "solidity-coverage": "^0.8.16",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5"
  }
}
